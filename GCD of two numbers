package arpita;

import java.util.Scanner;

public class gcd {
	public static void main(String args[]){
		Scanner s = new Scanner(System.in);
		System.out.print("Enter number1:");
		int a = s.nextInt();
		System.out.print("Enter number2):");
		int b = s.nextInt();
		int gcd=1,i;
		for(i=1 ; i<=a && i<=b ; i++)
		{
			if(a%i==0 && b%i==0)
             gcd=i;
		}
		System.out.println("GCD of numbers is:"+gcd);
				
	}

}
